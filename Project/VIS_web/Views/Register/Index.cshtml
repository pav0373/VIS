@{
    ViewData["Title"] = "Registrace";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Result"]</h3>

<form method="POST">
    <label for="login">Login</label>
    <input type="text" id="login" name="login" placeholder="Login" />

    <br /><br />

    <label for="email">Email</label>
    <input type="text" id="email" name="email" placeholder="Email" />

    <br /><br />

    <label for="heslo">Heslo</label>
    <input type="password" id="heslo" name="heslo" placeholder="Heslo" />

    <br /><br />

    <label for="krestni">Krestni</label>
    <input type="text" id="krestni" name="krestni" placeholder="Krestni" />

    <br /><br />

    <label for="prijmeni">Prijmeni</label>
    <input type="text" id="prijmeni" name="prijmeni" placeholder="Prijmeni" />

    <br /><br />

    <label for="ulice">Ulice</label>
    <input type="text" id="ulice" name="ulice" placeholder="Ulice" />

    <br /><br />

    <label for="mesto">Mesto</label>
    <input type="text" id="mesto" name="mesto" placeholder="Mesto" />

    <br /><br />

    <label for="psc">PSC</label>
    <input type="text" id="psc" name="psc" placeholder="PSC" />

    <br /><br />

    <label for="tel">Telefon</label>
    <input type="text" id="tel" name="tel" placeholder="Telefon" />

    <br /><br />

    <input type="submit" value="Registrovat" />
</form>